{
  "name": "coding-bible",
  "version": "1.0.0",
  "description": "an app to store my coding knowledge",
  "homepage": "https://github.com/emmanuelplougoulm/coding-bible#readme",
  "bugs": {
    "url": "https://github.com/emmanuelplougoulm/coding-bible/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emmanuelplougoulm/coding-bible.git"
  },
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "build": "vite build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node dist/main",
    "start:dev": "vite",
    "start:debug": "vite --debug",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:integration": "vitest run src/core/application/use-cases/language/__tests__/language.use-cases.integration.spec.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "mongoose": "^8.15.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "mongodb-memory-server": "^10.1.4",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "vite": "^4.5.2",
    "vite-plugin-node": "^3.1.0",
    "vitest": "^3.1.4"
  }
}
